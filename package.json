{
    "name": "vite-template",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "local": "graphql-codegen && vite",
        "start": "npx serve -s dist",
        "lint": "eslint .",
        "preview": "vite preview",
        "format-code": "npx prettier --write .",
        "build": "tsc -b && graphql-codegen && vite build",
        "build-tailwind": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css",
        "git-pre-commit": "eslint . && npm run build-tailwind && npm run build  && npx prettier --write . && git add . ",
        "git-commit": "npm run git-pre-commit && git commit",
        "generate": "graphql-codegen"
    },
    "dependencies": {
        "@apollo/client": "^3.12.4",
        "@emotion/react": "^11.13.5",
        "@emotion/styled": "^11.13.5",
        "@graphql-codegen/typescript-operations": "^4.4.0",
        "@graphql-codegen/typescript-react-apollo": "^4.3.2",
        "@lottiefiles/dotlottie-react": "^0.11.0",
        "@mui/icons-material": "^6.1.10",
        "@mui/material": "^6.1.10",
        "@react-three/fiber": "^8.17.10",
        "@types/three": "^0.171.0",
        "@use-gesture/react": "^10.3.1",
        "apexcharts": "^4.2.0",
        "apexsankey": "^1.1.2",
        "apextree": "^1.3.0",
        "cesium": "^1.124.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "framer-motion": "^11.15.0",
        "jwt-decode": "^4.0.0",
        "lucide-react": "^0.468.0",
        "path": "^0.12.7",
        "react": "^18.3.1",
        "react-chrono": "^2.6.1",
        "react-content-loader": "^7.0.2",
        "react-dom": "^18.3.1",
        "react-drag-drop-files": "^2.4.0",
        "react-helmet-async": "^2.0.5",
        "react-lineto": "^3.3.0",
        "react-resizable-panels": "^2.1.7",
        "react-router-dom": "^7.0.2",
        "react-spring": "^9.7.4",
        "react-toastify": "^10.0.6",
        "resium": "^1.18.3",
        "tailwind-merge": "^2.5.5",
        "tailwindcss-animate": "^1.0.7",
        "three": "^0.171.0",
        "universal-cookie": "^7.2.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@graphql-codegen/cli": "^5.0.3",
        "@graphql-codegen/client-preset": "^4.5.1",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@types/node": "^22.10.1",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.15.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.14",
        "globals": "^15.12.0",
        "graphql": "^16.10.0",
        "prettier": "^3.4.2",
        "tailwindcss": "^3.4.16",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.15.0",
        "vite": "^6.0.1"
    }
}
